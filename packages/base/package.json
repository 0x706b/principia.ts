    "concurrently": "^6.5.1",
    "cpx": "^1.5.0",
    "prettier": "^2.5.1",
    "sort-package-json": "^1.53.1",
    "ts-node": "^10.4.0",
    "ts-patch": "^2.0.1",
    "typescript": "^4.5.4"
  },
