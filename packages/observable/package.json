{
  "name": "@principia/observable",
  "version": "0.23.0",
  "description": "Observable",
  "repository": {
    "type": "git",
    "url": "https://github.com/0x706b/principia.ts.git",
    "directory": "packages/observable"
  },
  "license": "MIT",
  "author": "Peter Krol <peter@peterkrol.com>",
  "exports": {
    ".": {
      "traced": {
        "import": "./dist/dist-traced/esm/index.js",
        "require": "./dist/dist-traced/cjs/index.js"
      },
      "import": "./dist/dist/esm/index.js",
      "require": "./dist/dist/cjs/index.js"
    },
    "./Observable/combinators": {
      "traced": {
        "import": "./dist/dist-traced/esm/Observable/combinators/index.js",
        "require": "./dist/dist-traced/cjs/Observable/combinators/index.js"
      },
      "import": "./dist/dist/esm/Observable/combinators/index.js",
      "require": "./dist/dist/cjs/Observable/combinators/index.js"
    },
    "./Observable/constructors": {
      "traced": {
        "import": "./dist/dist-traced/esm/Observable/constructors/index.js",
        "require": "./dist/dist-traced/cjs/Observable/constructors/index.js"
      },
      "import": "./dist/dist/esm/Observable/constructors/index.js",
      "require": "./dist/dist/cjs/Observable/constructors/index.js"
    },
    "./Observable": {
      "traced": {
        "import": "./dist/dist-traced/esm/Observable/index.js",
        "require": "./dist/dist-traced/cjs/Observable/index.js"
      },
      "import": "./dist/dist/esm/Observable/index.js",
      "require": "./dist/dist/cjs/Observable/index.js"
    },
    "./Scheduler": {
      "traced": {
        "import": "./dist/dist-traced/esm/Scheduler/index.js",
        "require": "./dist/dist-traced/cjs/Scheduler/index.js"
      },
      "import": "./dist/dist/esm/Scheduler/index.js",
      "require": "./dist/dist/cjs/Scheduler/index.js"
    },
    "./*": {
      "traced": {
        "import": "./dist/dist-traced/esm/*",
        "require": "./dist/dist-traced/cjs/*.js"
      },
      "import": "./dist/dist/esm/*",
      "require": "./dist/dist/cjs/*.js"
    }
  },
  "main": "./dist/dist/cjs/index.js",
  "module": "./dist/dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "autofix": "yarn lint --fix",
    "build": "concurrently \"yarn build:types\" \"yarn build:esm\" \"yarn build:cjs\"",
    "build:cjs": "tsc --build ./config/tsconfig.cjs.json",
    "build:esm": "tsc --build ./config/tsconfig.esm.json && babel dist/dist/esm/ -d dist/dist/esm/ -s --plugins annotate-pure-calls",
    "build:postprocess": "yarn ts-node ../../scripts/rewrite-source-maps.ts && yarn ts-node ../../scripts/package.ts && yarn sort-package-json ./dist/package.json && yarn prettier --write ./dist/package.json && cpx ./fluent/**/*.d.ts ./dist/fluent",
    "build:test": "tsc --build ./config/tsconfig.test.json --verbose",
    "build:traced": "concurrently \"yarn build:traced:esm\" \"yarn build:traced:cjs\"",
    "build:traced:cjs": "tsc --build ./config/tsconfig.traced.cjs.json",
    "build:traced:esm": "tsc --build ./config/tsconfig.traced.esm.json && babel dist/dist-traced/esm/ -d dist/dist-traced/esm/ -s --plugins annotate-pure-calls",
    "build:types": "tsc --build ./config/tsconfig.types.json",
    "buildAll": "concurrently \"yarn build\" \"yarn build:traced\" && yarn build:postprocess",
    "clean": "rimraf dist build tsbuildinfo",
    "clean:test": "rimraf build",
    "generateSubpaths": "ts-node ../../scripts/subpath.ts && sort-package-json && prettier --write package.json",
    "lint": "eslint ."
  },
  "config": {},
  "dependencies": {
    "@principia/base": "^0.18.0",
    "@principia/compile": "^0.19.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "gitHead": "e41ff93cb1d2a09ef3996aea456303fa2798461e"
}
